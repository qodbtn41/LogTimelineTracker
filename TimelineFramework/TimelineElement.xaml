<UserControl x:Class="TimelineFramework.TimelineElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_SO__Slider_Thingy"
             mc:Ignorable="d">
    <Canvas x:Name="mainCanvas" Width="5" VerticalAlignment="Stretch">
        <Rectangle x:Name="rectOuter" Fill="#FF192E8B" Width="5" Opacity="0.3" VerticalAlignment="Stretch"/>
        <Rectangle x:Name="rectInner" Fill="#FF1530AA" Canvas.Left="2" Width="1" VerticalAlignment="Stretch"/>
    </Canvas>
</UserControl>
